javascript:void(/* v0.1.3 */(()=>{let data,qs=(n,e=document)=>e.querySelector(n),qsa=(n,e=document)=>Array.from(e.querySelectorAll(n)),scripts=qsa('script[type="application/ld+json"]'),host=location.host.replaceAll(/^www\./g,"");n:for(let n of scripts)try{let e=JSON.parse(n.innerHTML);console.log("Parsed script",e),Array.isArray(e["@type"])&&(e["@type"]=e["@type"][0]),e["@type"]&&"recipe"===e["@type"].toLowerCase()||!e["@graph"]||(e=e["@graph"]),Array.isArray(e)||(e=[e]);for(let n of e)if(console.log("Checking schema",n),Array.isArray(n["@type"])&&(n["@type"]=n["@type"][0]),"recipe"===n["@type"].toLowerCase()){if(console.log("Recipe found",n),Array.isArray(n.image)){let e=n.image[0];n.image=e.url||e}else n.image=n.image.url||n.image;Array.isArray(n.recipeInstructions)||(n.recipeInstructions=n.recipeInstructions.split(/\n+/g).map((n=>({text:n})))),data=n;break n}}catch(n){console.log(n)}if(!data){let n=qs('[itemtype="http://schema.org/Recipe"]')||qs('[itemtype="https://schema.org/Recipe"]');n&&(data={name:qs('[itemprop="name"]',n).innerText,description:"",image:qs('meta[itemprop="image"]')?.content||qs('[property="og:image"]')?.content,recipeIngredient:qsa('[itemprop="recipeIngredient"],[itemprop="ingredient"]',n).map((n=>n.innerText)),recipeInstructions:qsa('li[itemprop="recipeInstructions"],[itemprop="recipeInstructions"] p,[itemprop="recipeInstructions"] li,[itemprop="instructions"] p',n).filter((n=>!n.querySelector("*"))).map((n=>({text:n.innerText})))})}if(data)if("delish.com"===host)data.recipeInstructions=qsa('div[class="direction-lists"] li').map((n=>({text:n.innerText})));else if("bettycrocker.com"===host){let n=qsa(".recipePartIngredient");n=n.length?n:qsa(".ingredients-item-name"),data.recipeIngredient=n.map((n=>n.innerText)).filter((n=>n&&!n.toLowerCase().includes("advertisement")&&!n.toLowerCase().includes("add all ingredients to list"))),data.recipeInstructions=qsa(".recipePartStep .recipePartStepDescription").map((n=>({text:n.innerText})))}else"epicuri